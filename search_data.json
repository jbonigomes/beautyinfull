---
layout: null
---
{% assign posts=site.posts | where:"lang", page.lang %}
{
  {% for post in posts %}
    "{{ post.url | slugify }}": {
      "title": "{{ post.title | xml_escape }}",
      "content": "{{ post.content | strip_html | strip_newlines }}",
      "url": " {{ post.url | xml_escape }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
}
